@model List<JwtApp.Frontcs.Models.CategoryListResponseModel>
@{
    Layout = "_Layout";
}
<div class="container my-4 pt-4">
    <table class="table table-striped table-bordered">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Definition</th>
            </tr>
        </thead>
        <tbody>
            @if(Model?.Count > 0)
            {
                foreach (var item in Model)
                {
                <tr>
                    <th>@item.Id</th>
                    <td>@item.Definition</td>
                </tr>
                }
            }
            else
            {
                <tr>
                    <td colspan="2">Herhangi bir data bulunamadı</td>
                </tr>
            }
        </tbody>
    </table>
</div>